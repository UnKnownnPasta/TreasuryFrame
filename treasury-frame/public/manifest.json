{
	"manifest_version": 1,
	"type": "WebApp",
	"meta": {
		"name": "Test App",
		"version": "0.1.0",
		"minimum-overwolf-version": "0.120.0",
		"author": "someone",
		"icon": "./img/TreasuryFrame-hover.png",
		"icon_gray": "./img/TreasuryFrame-300.png",
		"launcher_icon": "TreasuryFrame.ico",
		"window_icon": "./img/TreasuryFrame-300.png",
		"description": "Lorem Ipsum is simply dummy text of the printing and typesetting industry."
	},
	"permissions": ["GameInfo"],
	"data": {
		"game_targeting": {
			"type": "dedicated",
			"game_ids": [8954]
		},
		"launch_events": [
			{
				"event": "GameLaunch",
				"event_data": {
					"game_ids": [8954]
				},
				"start_minimized": true
			}
		],
		"game_events": [8954],
		"start_window": "background",
		"windows": {
			"background": {
				"file": "./index.html",
				"background_optimization": false,
				"is_background_page": true
			},
			"desktop": {
				"file": "./index.html",
				"native_window": true,
				"transparent": true,
				"resizable": true,
				"size": {
					"width": 1212,
					"height": 699
				},
				"min_size": {
					"width": 1212,
					"height": 699
				},
				"max_size": {
					"width": 1920,
					"height": 1080
				},
				"desktop_only": true,
				"block_top_window_navigation": true,
				"popup_blocker": true,
				"mute": true
			},
			"in_game": {
				"file": "./index.html",
				"in_game_only": true,
				"focus_game_takeover": "ReleaseOnHidden",
				"block_top_window_navigation": true,
				"transparent": true,
				"resizable": true,
				"size": {
					"width": 350,
					"height": 450
				},
				"min_size": {
					"width": 350,
					"height": 450
				},
				"max_size": {
					"width": 350,
					"height": 450
				},
				"start_position": {
					"top": 200,
					"left": 12
				}
			}
		},
		"force_browser": "user",
		"developer": {
			"enable_auto_refresh": true,
			"reload_delay": 1000,
			"filter": "*.*"
		}
	}
}
