{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Test App",
        "version": "0.1.0",
        "minimum-overwolf-version": "0.120.0",
        "author": "someone",
        "icon": "TreasuryFrame-300.png",
        "icon_gray": "TreasuryFrame-hover.png",
        "launcher_icon": "TreasuryFrame.ico",
        "window_icon": "TreasuryFrame-300.png",
        "description": "Lorem Ipsum is simply dummy text of the printing and typesetting industry."
    },
    "permissions": ["Extensions", "Streaming", "Profile", "GameInfo"],
    "data": {
        "game_targeting": {
            "type": "dedicated",
            "game_ids": [8954]
        },
        "launch_events": [
            {
                "event": "GameLaunch",
                "event_data": {
                    "game_ids": [8954]
                },
                "start_minimized": true
            }
        ],
        "game_events": [8954],
        "start_window": "desktop",
        "windows": {
            "desktop": {
                "file": "./index.html",
                "native_window": true,
                "transparent": true,
                "resizable": true,
                "size": {
                    "width": 1212,
                    "height": 699
                },
                "min_size": {
                    "width": 1212,
                    "height": 699
                },
                "max_size": {
                    "width": 1920,
                    "height": 1080
                },
                "desktop_only": true,
                "block_top_window_navigation": true,
                "popup_blocker": true,
                "mute": true
            }
        },
        "force_browser": "user",
        "developer": {
            "enable_auto_refresh": true,
            "reload_delay": 1000,
            "filter": "*.*"
        }
    }
}
